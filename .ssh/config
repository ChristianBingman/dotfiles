Include ~/.ssh/*.local
Host *
  PubkeyAcceptedKeyTypes=+ssh-rsa
  HostKeyAlgorithms=+ssh-rsa

Host hulk nickfury ironman wolverine professorx captainamerica daredevil daredevil-fast
  HostName %h.christianbingman.com

Host *.christianbingman.com
  User christian
  ForwardAgent yes
  AddKeysToAgent yes
